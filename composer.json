{
    "name": "publishlayer/laravel-connector",
    "description": "Laravel connector for PublishLayer",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "illuminate/http": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0",
        "phpunit/phpunit": "^10.5|^11.5"
    },
    "autoload": {
        "psr-4": {
            "PublishLayer\\LaravelConnector\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PublishLayer\\LaravelConnector\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PublishLayer\\LaravelConnector\\PublishLayerServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
